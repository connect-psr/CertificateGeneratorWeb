<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         version="3.1">
    <servlet>
        <servlet-name>GenerateCertificateServlet</servlet-name>
        <servlet-class>com.certificate.GenerateCertificateServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>GenerateCertificateServlet</servlet-name>
        <url-pattern>/generate</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>form.jsp</welcome-file>
    </welcome-file-list>
</web-app>
